"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve 5 years of daily price data for 2-year Treasury futures to compute yields\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"template\": \"SELECT date, price FROM market_data WHERE symbol = :symbol AND date BETWEEN :start_date AND :end_date ORDER BY date ASC\",\n      \"params\": {\n        \"symbol\": \"2Y\",\n        \"start_date\": \"2019-06-01\",\n        \"end_date\": \"2024-06-01\"\n      },\n      \"columns\": [\"date\", \"price\"],\n      \"limit\": null\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve 5 years of daily price data for 10-year Treasury futures to compute yields\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"template\": \"SELECT date, price FROM market_data WHERE symbol = :symbol AND date BETWEEN :start_date AND :end_date ORDER BY date ASC\",\n      \"params\": {\n        \"symbol\": \"10Y\",\n        \"start_date\": \"2019-06-01\",\n        \"end_date\": \"2024-06-01\"\n      },\n      \"columns\": [\"date\", \"price\"],\n      \"limit\": null\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute the daily 2s10s spread from the retrieved 2Y and 10Y yield price data\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"compute_spread\",\n      \"spread_name\": \"2s10s\",\n      \"inputs\": {\n        \"2Y_data_task\": \"task_1\",\n        \"10Y_data_task\": \"task_2\"\n      }\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Search Polymarket for prediction markets related to recession odds\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"recession odds\",\n      \"limit\": 5,\n      \"session_id\": \"session_2s10s_recession\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Fetch historical price series for top relevant Polymarket recession odds markets to get recession probability time series\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"recession odds\",\n      \"session_id\": \"session_2s10s_recession\",\n      \"limit\": 3,\n      \"fields\": [\"price\", \"date\"],\n      \"time_frame\": \"last_5_years\"\n    },\n    \"dependencies\": [\"task_4\"]\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Perform a web search for recent macro commentary explaining flatness of 2s10s spread\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"flat 2s10s Treasury yield curve explanation\",\n      \"time_window\": \"past 3 months\"\n    },\n    \"dependencies\": []\n  }\n]"