"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve last 12 months of FOMC rate decision event data including dates, expected and actual rates, and surprise classification to anchor analysis timeline.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 12 months\",\n      \"event_type\": \"FOMC\",\n      \"fields\": [\"date\", \"expected_rate\", \"actual_rate\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily yield data for 2-year US Treasury note futures (symbol: 2Y) for the last 12 months anchored on FOMC event dates.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"2Y\",\n      \"fields\": [\"yield_close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-365, 0]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily yield data for 10-year US Treasury note futures (symbol: 10Y) for the last 12 months anchored on FOMC event dates.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y\",\n      \"fields\": [\"yield_close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-365, 0]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Compute the 2s10s spread (10Y yield minus 2Y yield) daily for the last 12 months based on yields retrieved in tasks 2 and 3.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"inputs\": [\"task_2\", \"task_3\"],\n      \"operation\": \"compute_daily_spread\",\n      \"spread_definition\": \"10Y yield minus 2Y yield\"\n    },\n    \"dependencies\": [\"task_2\", \"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Search Polymarket for prediction markets related to US recession odds or recession risk for the past year.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"US recession odds OR recession risk\",\n      \"limit\": 5\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Retrieve current prices, volumes, and liquidity data for the top 5 relevant Polymarket recession prediction markets identified in task 5.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"US recession odds OR recession risk\",\n      \"limit\": 5,\n      \"fields\": [\"price\", \"volume\", \"liquidity\"],\n      \"time_frame\": \"current\"\n    },\n    \"dependencies\": [\"task_5\"]\n  },\n  {\n    \"id\": \"task_7\",\n    \"description\": \"Fetch historical price series for the top 5 Polymarket recession prediction markets over the last 12 months for trend analysis.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"US recession odds OR recession risk\",\n      \"limit\": 5,\n      \"time_frame\": {\n        \"start\": \"12 months ago\",\n        \"end\": \"today\"\n      },\n      \"fields\": [\"price\"]\n    },\n    \"dependencies\": [\"task_5\"]\n  },\n  {\n    \"id\": \"task_8\",\n    \"description\": \"Extract trader sentiment and qualitative messages related to 2s10s curve flatness, recession risk, or curve inversion from Bloomberg Chat and Telegram for 12 months around FOMC event dates.\",\n    \"agent\": \"message_puller_agent\",\n    \"params\": {\n      \"channels\": [\"Bloomberg Chat\", \"Telegram\"],\n      \"keywords\": [\"2s10s\", \"curve flat\", \"recession\", \"yield curve inversion\", \"recession odds\"],\n      \"time_window\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-365, 0]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_9\",\n    \"description\": \"Combine the 2s10s spread history, recession odds from Polymarket, and trader sentiment data for comparative historical analysis of curve flatness vs recession risk and predictive market signals.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"inputs\": [\"task_4\", \"task_6\", \"task_7\", \"task_8\"],\n      \"operation\": \"integrate_and_compare_timeseries\"\n    },\n    \"dependencies\": [\"task_4\", \"task_6\", \"task_7\", \"task_8\"]\n  }\n]"