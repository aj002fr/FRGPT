"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve daily historical prices for 2-year and 10-year Treasury futures to compute the 2s10s yield spread over the past 2 years.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"template\": \"SELECT date, symbol, close_price FROM market_data WHERE symbol IN ('2Y', '10Y') AND date >= DATE('now', '-2 years') ORDER BY date ASC\",\n      \"fields\": [\"date\", \"symbol\", \"close_price\"],\n      \"frequency\": \"daily\",\n      \"instrument\": \"2Y,10Y\",\n      \"date_range\": {\"start\": \"2 years ago\", \"end\": \"today\"}\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Search Polymarket for markets related to recession odds or recession probability to identify relevant prediction markets.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"recession odds OR recession probability\",\n      \"limit\": 5\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Fetch historical price series for the top recession-related market(s) found in Task 2 for the past 2 years to analyze evolution of recession odds.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"use market ids from task_2 output\",\n      \"time_frame\": \"last 2 years\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Retrieve current prices and volumes for the top recession-related markets found in Task 2 to show current prediction market recession odds.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"use market ids from task_2 output\",\n      \"time_frame\": \"current\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Search the web for recent macroeconomic commentary explaining why the 2s10s Treasury yield curve is flat, focusing on recession risks and market expectations.\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"flat 2s10s yield curve recession risk\",\n      \"time_window\": \"past 3 months\"\n    },\n    \"dependencies\": []\n  }\n]"