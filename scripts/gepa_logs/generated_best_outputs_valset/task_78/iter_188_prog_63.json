"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve FOMC rate decision events for the last 5 years to anchor macro environment and recessions\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"FOMC\",\n      \"count_or_range\": \"last 5 years\",\n      \"fields\": [\"date\", \"rate_change\", \"actual\", \"expected\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily price data for 2-year Treasury futures (2Y) for the last 5 years as yield proxy\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"2Y\",\n      \"fields\": [\"close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"relative\": [-1825, 0]\n      }\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily price data for 10-year Treasury futures (10Y) for the last 5 years as yield proxy\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y\",\n      \"fields\": [\"close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"relative\": [-1825, 0]\n      }\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Compute 2s10s spread history as the difference between 10Y and 2Y yield proxies (inverse of prices)\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_2Y_prices\": \"task_2\",\n      \"input_10Y_prices\": \"task_3\"\n    },\n    \"dependencies\": [\"task_2\", \"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Search Polymarket for prediction markets related to recession odds, 2s10s curve inversion, or yield curve flattening\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"recession odds OR 2s10s OR yield curve inversion OR flattening\",\n      \"limit\": 5\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Retrieve current market data (prices, volumes, liquidity) for the top relevant Polymarket markets found\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"recession odds OR 2s10s OR yield curve inversion OR flattening\",\n      \"fields\": [\"prices\", \"volumes\", \"liquidity\"],\n      \"limit\": 5\n    },\n    \"dependencies\": [\"task_5\"]\n  },\n  {\n    \"id\": \"task_7\",\n    \"description\": \"Retrieve historical price series for the selected Polymarket prediction markets over the past 180 days\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"recession odds OR 2s10s OR yield curve inversion OR flattening\",\n      \"time_frame\": {\n        \"relative\": [-180, 0]\n      },\n      \"limit\": 5\n    },\n    \"dependencies\": [\"task_5\"]\n  },\n  {\n    \"id\": \"task_8\",\n    \"description\": \"Analyze repricing and trend evolution in selected prediction markets for recession odds and curve inversion\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"historical_prices_task\": \"task_7\",\n      \"current_data_task\": \"task_6\"\n    },\n    \"dependencies\": [\"task_6\", \"task_7\"]\n  }\n]"