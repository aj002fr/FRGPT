"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve the last 20 Nonfarm Payroll (NFP) events with dates, expected values (consensus), actual values, and surprise metrics (including standard deviations).\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": 20,\n      \"event_type\": \"NFP\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise_std\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily 2-year Treasury yield data around the 20 NFP event dates for calculating yield moves.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 1]\n      },\n      \"fields\": [\"price\", \"yield\"],\n      \"frequency\": \"daily\",\n      \"instrument\": \"2Y\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily 10-year Treasury yield data around the 20 NFP event dates for calculating yield moves.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 1]\n      },\n      \"fields\": [\"price\", \"yield\"],\n      \"frequency\": \"daily\",\n      \"instrument\": \"10Y\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate 2-year and 10-year Treasury yield moves around each NFP event date based on the retrieved yield data.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"calculation\": \"yield_moves\",\n      \"event_dates_task\": \"task_1\",\n      \"yield_data_2y_task\": \"task_2\",\n      \"yield_data_10y_task\": \"task_3\"\n    },\n    \"dependencies\": [\"task_2\", \"task_3\"]\n  }\n]"