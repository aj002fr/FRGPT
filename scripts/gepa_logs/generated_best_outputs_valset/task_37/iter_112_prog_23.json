"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve the last 20 Nonfarm Payroll (NFP) events with actual, expected, and surprise data (including standard deviation surprise).\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 20\",\n      \"event_type\": \"NFP\",\n      \"fields\": [\"actual\", \"expected\", \"surprise\", \"surprise_stddev\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve 2-year Treasury yield data around each NFP event date to calculate yield moves.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"2Y Treasury Note\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 1]\n      },\n      \"fields\": [\"yield\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve 10-year Treasury yield data around each NFP event date to calculate yield moves.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y Treasury Note\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 1]\n      },\n      \"fields\": [\"yield\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate 2Y and 10Y yield moves around each NFP event date using the yield data retrieved from tasks 2 and 3.\",\n    \"agent\": \"workflow_orchestrator\",\n    \"params\": {},\n    \"dependencies\": [\"task_2\", \"task_3\"]\n  }\n]"