"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve the last 20 Nonfarm Payroll (NFP) events with event dates, expected (consensus) values, actual values, and surprise in standard deviations.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 20\",\n      \"event_type\": \"NFP\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\", \"surprise_stddev\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily 2-year Treasury futures or notes prices for a window from 1 day before to 1 day after each NFP event date to capture yield moves.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"2Y Treasury futures or notes\",\n      \"fields\": [\"price\", \"yield\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 1]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily 10-year Treasury futures or notes prices for a window from 1 day before to 1 day after each NFP event date to capture yield moves.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y Treasury futures or notes\",\n      \"fields\": [\"price\", \"yield\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 1]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Compute yield moves for 2Y and 10Y Treasury instruments around each NFP event by comparing prices/yields before and after the event dates.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"calculate yield moves\",\n      \"inputs\": {\n        \"nfp_events\": \"task_1\",\n        \"prices_2y\": \"task_2\",\n        \"prices_10y\": \"task_3\"\n      }\n    },\n    \"dependencies\": [\"task_1\", \"task_2\", \"task_3\"]\n  }\n]"