"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve recent CPI event data (dates, expected and actual values) to anchor U.S. inflation events.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"CPI\",\n      \"count_or_range\": \"last 5 events\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve recent FOMC event data (dates, expected and actual values) to anchor Fed policy events.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"FOMC\",\n      \"count_or_range\": \"last 5 events\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Search Polymarket for current contracts related to U.S. inflation and Fed policy.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. inflation and Federal Reserve policy\",\n      \"limit\": 20\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Fetch current market data (prices, volumes, liquidity) for Polymarket contracts found related to inflation and Fed policy.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. inflation and Federal Reserve policy\",\n      \"fields\": [\"prices\", \"volumes\", \"liquidity\"],\n      \"limit\": 20\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Fetch historical price series for Polymarket contracts related to inflation and Fed policy anchored around CPI events.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. inflation and Federal Reserve policy\",\n      \"time_frame\": {\n        \"date_range\": {\n          \"anchor_task\": \"task_1\",\n          \"anchor_field\": \"date\",\n          \"relative\": [-10, 5]\n        }\n      }\n    },\n    \"dependencies\": [\"task_1\", \"task_3\"]\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Fetch historical price series for Polymarket contracts related to inflation and Fed policy anchored around FOMC events.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. inflation and Federal Reserve policy\",\n      \"time_frame\": {\n        \"date_range\": {\n          \"anchor_task\": \"task_2\",\n          \"anchor_field\": \"date\",\n          \"relative\": [-10, 5]\n        }\n      }\n    },\n    \"dependencies\": [\"task_2\", \"task_3\"]\n  }\n]"