"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve recent CPI (inflation) macroeconomic event dates, expected and actual values, and surprises for anchoring Polymarket contract analysis.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"CPI\",\n      \"count_or_range\": \"last 10\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve recent FOMC (Fed policy) macroeconomic event dates, expected and actual values, and surprises for anchoring Polymarket contract analysis.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"FOMC\",\n      \"count_or_range\": \"last 10\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Search Polymarket markets relevant to U.S. inflation and Fed policy using natural language queries to identify active contracts.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. inflation and Federal Reserve policy\",\n      \"limit\": 20\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Fetch current market prices, volumes, and liquidity for Polymarket contracts identified in task_3.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. inflation and Federal Reserve policy\",\n      \"fields\": [\"current_prices\", \"24h_volume\", \"7d_volume\", \"liquidity\"],\n      \"limit\": 20\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Fetch historical price series for Polymarket contracts from task_3, anchored on CPI and FOMC event dates from task_1 and task_2 to analyze repricing around macro events.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. inflation and Federal Reserve policy\",\n      \"time_frame\": {\n        \"anchor_tasks\": [\"task_1\", \"task_2\"],\n        \"anchor_field\": \"date\",\n        \"relative\": [-7, 7]\n      },\n      \"limit\": 20,\n      \"fields\": [\"historical_prices\", \"historical_volumes\"]\n    },\n    \"dependencies\": [\"task_3\", \"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Analyze repricing and trend evolution of Polymarket contracts related to U.S. inflation and Fed policy using current and historical data from tasks 4 and 5.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. inflation and Federal Reserve policy\",\n      \"current_data_task\": \"task_4\",\n      \"historical_data_task\": \"task_5\"\n    },\n    \"dependencies\": [\"task_4\", \"task_5\"]\n  }\n]"