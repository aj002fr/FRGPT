"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve all U.S. Nonfarm Payroll (NFP) event dates since 1990\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"NFP\",\n      \"count_or_range\": {\"start_date\": \"1990-01-01\", \"end_date\": \"2024-06-30\"},\n      \"fields\": [\"date\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Fetch daily closing prices for 10-year Treasury futures around each NFP event date and the previous trading day\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\", \n      \"fields\": [\"last_price\", \"date\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 0]\n      },\n      \"columns\": [\"last_price\", \"date\"],\n      \"limit\": 5000\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute daily yield move classification (higher, lower, unchanged) for each NFP Friday based on closing prices\",\n    \"agent\": \"custom_analysis\",\n    \"params\": {\n      \"input_prices_task\": \"task_2\",\n      \"input_dates_task\": \"task_1\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Visualize the distribution of 10-year yield moves (higher vs lower vs unchanged) on NFP Fridays since 1990\",\n    \"agent\": \"custom_visualization\",\n    \"params\": {\n      \"input_classification_task\": \"task_3\"\n    },\n    \"dependencies\": [\"task_3\"]\n  }\n]"