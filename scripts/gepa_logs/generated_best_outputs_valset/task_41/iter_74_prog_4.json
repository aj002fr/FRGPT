"[\n  {\n    \"task_id\": \"task_1_get_nfp_dates\",\n    \"agent\": \"event_data_puller_agent\",\n    \"description\": \"Retrieve all U.S. Nonfarm Payroll (NFP) event dates since 1990.\",\n    \"dependencies\": [],\n    \"params\": {\n      \"event_type\": \"NFP\",\n      \"count_or_range\": {\"start_year\": 1990, \"end_year\": 2024},\n      \"fields\": [\"date\"]\n    }\n  },\n  {\n    \"task_id\": \"task_2_get_10y_closes\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Retrieve daily closing prices of 10-year Treasury futures (TY) for each NFP date and the previous trading day.\",\n    \"dependencies\": [\"task_1_get_nfp_dates\"],\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1_get_nfp_dates\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 0]\n      },\n      \"fields\": [\"close\"],\n      \"frequency\": \"daily\"\n    }\n  },\n  {\n    \"task_id\": \"task_3_compute_price_moves\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Compute if 10-year Treasury futures closing price on NFP Fridays closed higher, lower, or unchanged compared to previous trading day.\",\n    \"dependencies\": [\"task_2_get_10y_closes\"],\n    \"params\": {\n      \"input_data\": {\n        \"from_task\": \"task_2_get_10y_closes\"\n      }\n    }\n  },\n  {\n    \"task_id\": \"task_4_aggregate_move_frequencies\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Aggregate counts of closes higher vs lower vs unchanged on NFP Fridays since 1990.\",\n    \"dependencies\": [\"task_3_compute_price_moves\"],\n    \"params\": {\n      \"input_data\": {\n        \"from_task\": \"task_3_compute_price_moves\"\n      }\n    }\n  },\n  {\n    \"task_id\": \"task_5_visualize_distribution\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Visualize the distribution of 10-year Treasury futures closing price moves (higher, lower, unchanged) on NFP Fridays since 1990.\",\n    \"dependencies\": [\"task_4_aggregate_move_frequencies\"],\n    \"params\": {\n      \"input_data\": {\n        \"from_task\": \"task_4_aggregate_move_frequencies\"\n      },\n      \"chart_type\": \"bar_chart\",\n      \"title\": \"Distribution of 10Y Treasury Futures Closing Price Moves on NFP Fridays Since 1990\",\n      \"x_axis\": \"Move Type\",\n      \"y_axis\": \"Frequency\"\n    }\n  }\n]"