"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve the last 3 key macroeconomic events (e.g., CPI, NFP, FOMC) with their dates and key data.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 3\",\n      \"event_type\": [\"CPI\", \"NFP\", \"FOMC\"],\n      \"fields\": [\"date\", \"event_type\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Search the web for recent news articles related to the retrieved macro events around their dates.\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"macroeconomic news CPI NFP FOMC rates inflation\",\n      \"time_window\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-2, 2]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve trader chat messages from relevant channels filtered by keywords (CPI, NFP, FOMC, rates, inflation) around the macro event dates.\",\n    \"agent\": \"message_puller_agent\",\n    \"params\": {\n      \"channels\": [\"Bloomberg Chat\", \"Telegram\"],\n      \"keywords\": [\"CPI\", \"NFP\", \"FOMC\", \"rates\", \"inflation\"],\n      \"time_window\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-2, 2]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  }\n]"