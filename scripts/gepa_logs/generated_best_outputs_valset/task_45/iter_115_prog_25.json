"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve FOMC meeting event data for the last 10 years including dates, actual rate changes, expected values, and surprise metrics.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"FOMC\",\n      \"count_or_range\": \"last 10 years\",\n      \"fields\": [\"date\", \"actual_rate_change\", \"expected_rate_change\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Identify first pause dates after hiking cycles from the retrieved FOMC event data (conceptual data processing step).\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_event_task\": \"task_1\",\n      \"logic\": \"Detect sequences of consecutive rate hikes, then find the first FOMC meeting with no rate hike (pause) immediately after.\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily TY futures price data for the 3 months (90 days) following each identified first pause date, anchored on the pause date.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"close\", \"open\", \"high\", \"low\", \"volume\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"pause_date\",\n        \"relative\": [0, 90]\n      }\n    },\n    \"dependencies\": [\"task_2\"]\n  }\n]"