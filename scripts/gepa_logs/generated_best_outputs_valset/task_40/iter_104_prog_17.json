"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve past 12 months of core CPI events including dates, actual, expected, and surprise classification\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 12 months\",\n      \"event_type\": \"core CPI\",\n      \"fields\": [\"date\", \"actual\", \"expected\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Filter core CPI events from task_1 to include only positive surprise events (actual > expected)\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"filter\": \"positive surprise\",\n      \"input_event_task\": \"task_1\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Fetch daily TY futures price data for date windows anchored on positive surprise core CPI event dates from task_2, for 1 day before to 1 day after each event date\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"last_price\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 1]\n      }\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate daily returns of TY futures using price data from task_3 to prepare data for histogram plotting\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"input_price_task\": \"task_3\",\n      \"calculation\": \"daily returns\"\n    },\n    \"dependencies\": [\"task_3\"]\n  }\n]"