"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve past core CPI event data with actual, expected values and surprise classification.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 20 events\",\n      \"event_type\": \"core CPI\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\", \"surprise_label\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily TY futures prices for a window around core CPI event dates (event day and the day before and after).\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"price_close\", \"date\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 1]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute daily returns for TY futures from price_close data.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_task\": \"task_2\",\n      \"price_field\": \"price_close\",\n      \"output_field\": \"daily_return\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Filter daily TY returns to only those on days with positive core CPI surprise (actual > expected).\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"returns_task\": \"task_3\",\n      \"event_data_task\": \"task_1\",\n      \"filter_condition\": \"actual > expected\"\n    },\n    \"dependencies\": [\"task_3\", \"task_1\"]\n  }\n]"