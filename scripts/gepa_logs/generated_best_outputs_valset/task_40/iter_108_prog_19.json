"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve historical core CPI events with expected, actual values and label upside surprises\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 5 years\",\n      \"event_type\": \"core CPI\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Filter core CPI events to only those with upside surprise (actual > expected) to get event dates\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"filter\": \"surprise == 'positive'\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily closing prices for TY futures on dates of upside core CPI surprises\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"date\", \"close\"],\n      \"date_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"date\",\n        \"relative\": [0, 0]\n      },\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Compute daily TY returns for the days of upside core CPI surprises\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"calculation\": \"daily returns from close prices\",\n      \"input_task\": \"task_3\",\n      \"input_field\": [\"date\", \"close\"]\n    },\n    \"dependencies\": [\"task_3\"]\n  }\n]"