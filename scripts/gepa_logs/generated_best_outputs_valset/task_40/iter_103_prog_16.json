"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve past core CPI events with actual, expected values and label positive surprises\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 20 events\",\n      \"event_type\": \"core CPI\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Fetch daily closing prices for TY futures on dates of core CPI events\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"frequency\": \"daily\",\n      \"fields\": [\"last_price\"],\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [0, 0]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Calculate daily TY returns for days with positive core CPI surprises\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"calculate_returns\",\n      \"input_prices_task\": \"task_2\",\n      \"filter_surprise_task\": \"task_1\",\n      \"filter_condition\": \"actual > expected\"\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  }\n]"