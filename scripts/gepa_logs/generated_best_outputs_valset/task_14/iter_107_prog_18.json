"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve all FOMC meeting events in 2013 including dates, expected and actual values, and surprise classification to identify the Taper Tantrum period.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"FOMC\",\n      \"count_or_range\": \"events in 2013\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily 10-year Treasury futures (TY) market data for a window of 3 days before to 3 days after each 2013 FOMC event date (Taper Tantrum period).\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"frequency\": \"daily\",\n      \"fields\": [\"last_price\", \"volume\", \"bid\", \"ask\"],\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-3, 3]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily 2-year Treasury futures (2Y) market data for the same relative window around 2013 FOMC event dates to analyze curve moves during the Taper Tantrum.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"2Y\",\n      \"frequency\": \"daily\",\n      \"fields\": [\"last_price\", \"volume\", \"bid\", \"ask\"],\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-3, 3]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Retrieve trader messages from Bloomberg Chat and Telegram containing keywords 'taper', 'FOMC', 'Treasury', filtered for the time window around the 2013 FOMC events to capture market sentiment during the Taper Tantrum.\",\n    \"agent\": \"message_puller_agent\",\n    \"params\": {\n      \"channels\": [\"Bloomberg Chat\", \"Telegram\"],\n      \"keywords\": [\"taper\", \"FOMC\", \"Treasury\"],\n      \"time_window\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-3, 3]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Search the web for macro-relevant news articles and commentary about the 2013 Taper Tantrum and its impact on treasury markets and rates within a month around the event dates.\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"2013 Taper Tantrum impact on treasury markets and Federal Reserve announcements\",\n      \"time_window\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-30, 30]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  }\n]"