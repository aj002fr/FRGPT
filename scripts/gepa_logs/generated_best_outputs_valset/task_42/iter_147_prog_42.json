"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve daily closing prices for 10-year Treasury futures (TY) for the last 3 years\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"price_close\", \"timestamp\"],\n      \"frequency\": \"daily\",\n      \"date_range\": \"last 3 years\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily closing prices for 2-year Treasury futures (US) for the last 3 years\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"US\",\n      \"fields\": [\"price_close\", \"timestamp\"],\n      \"frequency\": \"daily\",\n      \"date_range\": \"last 3 years\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute the daily 2s10s spread as the difference between TY and US closing prices for each day\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_data_10Y\": \"task_1\",\n      \"input_data_2Y\": \"task_2\",\n      \"operation\": \"spread_computation\",\n      \"spread_definition\": \"TY price_close minus US price_close on matching dates\"\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate the daily changes (day-over-day differences) in the 2s10s spread time series\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_spread_series\": \"task_3\",\n      \"operation\": \"daily_difference\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Compute the distribution (frequency or histogram) of daily changes in the 2s10s spread\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_daily_changes\": \"task_4\",\n      \"operation\": \"distribution_computation\"\n    },\n    \"dependencies\": [\"task_4\"]\n  }\n]"