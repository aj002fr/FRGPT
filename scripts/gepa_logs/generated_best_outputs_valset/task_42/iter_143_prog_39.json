"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve daily closing price data for the 2-year Treasury futures (2Y) over the last 3 years.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"2Y\",\n      \"fields\": [\"price_close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": \"last 3 years\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily closing price data for the 10-year Treasury futures (10Y) over the last 3 years.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y\",\n      \"fields\": [\"price_close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": \"last 3 years\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute the daily 2s10s spread and its daily changes (day-over-day differences) from the closing prices retrieved in task_1 and task_2.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_2Y_prices_task\": \"task_1\",\n      \"input_10Y_prices_task\": \"task_2\",\n      \"calculation\": \"spread = 10Y_price_close - 2Y_price_close; daily_changes = day-over-day differences of spread\"\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  }\n]"