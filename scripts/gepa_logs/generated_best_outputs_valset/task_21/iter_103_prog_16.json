"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Search Polymarket for contracts related to 'fed funds rate at year-end' and retrieve current market prices, volumes, and liquidity\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"fed funds rate at year-end\",\n      \"limit\": 5\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Fetch historical price series for the Polymarket contracts identified in task_1 to construct implied path over time\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"fed funds rate at year-end\",\n      \"time_frame\": \"historical\",\n      \"limit\": 5\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Fetch current market prices again for the identified contracts to overlay with historical implied path\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"fed funds rate at year-end\",\n      \"time_frame\": \"current\",\n      \"limit\": 5\n    },\n    \"dependencies\": [\"task_1\"]\n  }\n]"