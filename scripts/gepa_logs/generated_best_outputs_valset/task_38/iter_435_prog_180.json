"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve current market prices (bid/ask/last) for all UST futures and options in the portfolio to enable mark-to-market valuation. Positions and cost basis are assumed provided externally.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"pattern matching all UST futures and options in the portfolio (user to specify exact symbols)\",\n      \"date_range\": \"latest available date or current day\",\n      \"fields\": [\"bid\", \"ask\", \"last\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Compute current portfolio P&L by comparing externally provided cost basis and positions versus current market prices from task_1. This is a processing task assuming portfolio data input is external.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"portfolio_positions\": \"externally provided\",\n      \"cost_basis\": \"externally provided\",\n      \"market_prices_task\": \"task_1\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Calculate portfolio Value at Risk (VaR) using current market prices and portfolio composition, referencing P&L from task_2. This is a processing task assuming risk model and parameters are external.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"portfolio_positions\": \"externally provided\",\n      \"market_prices_task\": \"task_1\",\n      \"pnl_task\": \"task_2\",\n      \"risk_model\": \"externally provided\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Recommend exact trade sizes to achieve a target Sharpe ratio based on current P&L and VaR, as computed in tasks 2 and 3. This is a processing/optimization task requiring external Sharpe ratio target and risk preferences.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"pnl_task\": \"task_2\",\n      \"var_task\": \"task_3\",\n      \"target_sharpe_ratio\": \"user specified\"\n    },\n    \"dependencies\": [\"task_3\"]\n  }\n]"