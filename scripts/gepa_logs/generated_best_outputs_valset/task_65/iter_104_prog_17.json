"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Fetch dates and details of NFP, CPI, and FOMC events from event_data_puller_agent for the last 10 years\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 10 years\",\n      \"event_type\": [\"NFP\", \"CPI\", \"FOMC\"],\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Fetch daily market data (price or yield) for 2-year Treasury instrument for the last 10 years\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": \"last 10 years\",\n      \"frequency\": \"daily\",\n      \"instrument\": \"2Y Treasury Note or future\",\n      \"fields\": [\"date\", \"last_price\", \"yield\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Fetch daily market data (price or yield) for 10-year Treasury instrument for the last 10 years\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": \"last 10 years\",\n      \"frequency\": \"daily\",\n      \"instrument\": \"10Y Treasury Note or future\",\n      \"fields\": [\"date\", \"last_price\", \"yield\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Compute daily moves of the 2s10s curve from 2Y and 10Y treasury daily yields\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"computation\": \"Calculate daily differences of (10Y yield - 2Y yield) to get daily moves in the 2s10s curve\",\n      \"input_data_tasks\": [\"task_2\", \"task_3\"]\n    },\n    \"dependencies\": [\"task_2\", \"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Prepare combined dataset of 2s10s daily moves and annotate with key event dates for plotting\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"daily_moves_task\": \"task_4\",\n      \"ev