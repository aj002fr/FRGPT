"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve intraday price data (high and low prices) for 10-year Treasury futures (TY) for the past 25 years\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY*\",\n      \"date_range\": {\n        \"start_date\": \"1999-06-01\",\n        \"end_date\": \"2024-06-01\"\n      },\n      \"fields\": [\"price\", \"bid\", \"ask\", \"volume\"],\n      \"frequency\": \"intraday\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Compute daily intraday range (high - low) from intraday price data and identify the top 10 days with the largest intraday range in TY futures\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"input_data_anchor\": \"task_1\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve all major macroeconomic events (CPI, NFP, FOMC, PCE, etc.) with dates and surprise metrics for the past 25 years\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 25 years\",\n      \"event_type\": \"CPI,NFP,FOMC,PCE\",\n      \"fields\": \"date,expected,actual,surprise\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Identify which macroeconomic events occurred on the top 10 largest intraday range days in TY futures\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"date_list_anchor\": \"task_2\",\n      \"event_type\": \"CPI,NFP,FOMC,PCE\",\n      \"fields\": \"date,event_type,expected,actual,surprise\"\n    },\n    \"dependencies\": [\"task_2\", \"task_3\"]\n  }\n]"