"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve 25 years of daily intraday data (high and low prices) for TY futures to compute intraday ranges.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": \"last 25 years\",\n      \"instrument\": \"TY futures\",\n      \"fields\": [\"high\", \"low\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Identify the top 10 days with the biggest intraday price ranges from the retrieved TY futures data.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"compute_intraday_range_and_rank\",\n      \"input_task\": \"task_1\",\n      \"top_n\": 10\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve macroeconomic events (CPI, NFP, FOMC, etc.) that occurred on the top 10 biggest intraday range days identified in TY futures.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"specific dates\",\n      \"event_type\": [\"CPI\", \"NFP\", \"FOMC\", \"PCE\", \"Other major macro events\"],\n      \"dates\": {\n        \"anchor_task\": \"task_2\",\n        \"field\": \"dates\"\n      },\n      \"fields\": [\"event_date\", \"event_name\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": [\"task_2\"]\n  }\n]"