"[\n  {\n    \"id\": \"task_1_retrieve_2y_data\",\n    \"description\": \"Retrieve daily close price data for the 2-year Treasury note over the last 5 years.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"2Y\",\n      \"fields\": [\"close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": \"last 5 years\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2_retrieve_10y_data\",\n    \"description\": \"Retrieve daily close price data for the 10-year Treasury note over the last 5 years.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y\",\n      \"fields\": [\"close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": \"last 5 years\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3_compute_2s10s_daily_moves\",\n    \"description\": \"Compute the daily 2s10s spread as the difference between 10Y and 2Y daily close prices, then calculate the distribution of daily changes in this spread.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"inputs\": {\n        \"2Y_data\": \"task_1_retrieve_2y_data\",\n        \"10Y_data\": \"task_2_retrieve_10y_data\"\n      },\n      \"calculation\": \"spread = 10Y_close - 2Y_close; daily_moves = daily_diff(spread)\"\n    },\n    \"dependencies\": [\"task_1_retrieve_2y_data\", \"task_2_retrieve_10y_data\"]\n  }\n]"