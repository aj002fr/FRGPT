"[\n  {\n    \"task_id\": \"task_1\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Retrieve daily price or yield data for the 2-year Treasury Note over the last 5 years\",\n    \"dependencies\": [],\n    \"params\": {\n      \"date_range\": \"last 5 years\",\n      \"frequency\": \"daily\",\n      \"instrument\": \"2-year Treasury Note\",\n      \"fields\": [\"close\", \"yield\"]\n    }\n  },\n  {\n    \"task_id\": \"task_2\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Retrieve daily price or yield data for the 10-year Treasury Note over the last 5 years\",\n    \"dependencies\": [],\n    \"params\": {\n      \"date_range\": \"last 5 years\",\n      \"frequency\": \"daily\",\n      \"instrument\": \"10-year Treasury Note\",\n      \"fields\": [\"close\", \"yield\"]\n    }\n  },\n  {\n    \"task_id\": \"task_3\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Compute the daily 2s10s spread by subtracting the 2-year yield from the 10-year yield for each day\",\n    \"dependencies\": [\"task_1\", \"task_2\"],\n    \"params\": {\n      \"computation\": \"daily 10-year yield minus daily 2-year yield\",\n      \"input_data_tasks\": [\"task_1\", \"task_2\"]\n    }\n  },\n  {\n    \"task_id\": \"task_4\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Compute the daily moves (daily changes) in the 2s10s spread from the computed spread time series\",\n    \"dependencies\": [\"task_3\"],\n    \"params\": {\n      \"computation\": \"daily difference of 2s10s spread from task_3\"\n    }\n  },\n  {\n    \"task_id\": \"task_5\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Prepare the distribution data of daily moves in the 2s10s spread for plotting the distribution\",\n    \"dependencies\": [\"task_4\"],\n    \"params\": {\n      \"computation\": \"distribution statistics and histogram data of daily 2s10s moves from task_4\"\n    }\n  }\n]"