"[\n  {\n    \"task_id\": \"fetch_2y_prices\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Retrieve daily closing prices for 2-year treasury futures over the last year.\",\n    \"dependencies\": [],\n    \"params\": {\n      \"template\": \"SELECT trade_date, close_price FROM market_data WHERE symbol LIKE '2Y%' AND trade_date >= CURRENT_DATE - INTERVAL '365 days' ORDER BY trade_date ASC\",\n      \"params\": {},\n      \"columns\": [\"trade_date\", \"close_price\"],\n      \"limit\": null\n    }\n  },\n  {\n    \"task_id\": \"fetch_10y_prices\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Retrieve daily closing prices for 10-year treasury futures over the last year.\",\n    \"dependencies\": [],\n    \"params\": {\n      \"template\": \"SELECT trade_date, close_price FROM market_data WHERE symbol LIKE '10Y%' AND trade_date >= CURRENT_DATE - INTERVAL '365 days' ORDER BY trade_date ASC\",\n      \"params\": {},\n      \"columns\": [\"trade_date\", \"close_price\"],\n      \"limit\": null\n    }\n  },\n  {\n    \"task_id\": \"compute_and_plot_2s10s_moves\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Compute the daily changes in the 2s10s curve from 2Y and 10Y prices and plot the distribution of daily moves.\",\n    \"dependencies\": [\"fetch_2y_prices\", \"fetch_10y_prices\"],\n    \"params\": {\n      \"operation\": \"compute_spread_and_daily_moves\",\n      \"input_data_tasks\": [\"fetch_2y_prices\", \"fetch_10y_prices\"],\n      \"spread_formula\": \"10Y_price - 2Y_price\",\n      \"output\": \"distribution_plot\"\n    }\n  }\n]"