"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve daily price or yield data for the 2-year Treasury instrument over the last 365 days\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"2Y Treasury\",\n      \"frequency\": \"daily\",\n      \"fields\": [\"price\", \"yield\"],\n      \"date_range\": \"last 365 days\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily price or yield data for the 10-year Treasury instrument over the last 365 days\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y Treasury\",\n      \"frequency\": \"daily\",\n      \"fields\": [\"price\", \"yield\"],\n      \"date_range\": \"last 365 days\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute the daily 2s10s spread by subtracting the 2Y yield from the 10Y yield using data from tasks 1 and 2\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"calculate spread\",\n      \"spread_name\": \"2s10s\",\n      \"input_data\": {\n        \"2Y\": \"task_1\",\n        \"10Y\": \"task_2\"\n      },\n      \"fields\": [\"yield\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate daily moves (day-over-day changes) of the 2s10s spread from task 3\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"daily differences\",\n      \"input_data\": \"task_3\",\n      \"field\": \"2s10s spread\",\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Prepare the distribution data of daily moves in the 2s10s curve for plotting\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"input_data\": \"task_4\",\n      \"operation\": \"distribution statistics\",\n      \"fields\": [\"daily moves\"]\n    },\n    \"dependencies\": [\"task_4\"]\n  }\n]"