"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve TY futures options market data (bid, ask, price, volume) for the last year to analyze historical implied volatility levels.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"template\": \"select * from market_data where symbol like 'TY%' and instrument_type='option' and date >= :start_date and date <= :end_date\",\n      \"params\": {\n        \"start_date\": \"2023-06-01\",\n        \"end_date\": \"2024-06-01\"\n      },\n      \"columns\": [\"date\", \"symbol\", \"bid\", \"ask\", \"last\", \"volume\"],\n      \"limit\": 10000\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve TY futures options market data (bid, ask, price, volume) for the recent month to analyze current implied volatility levels.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"template\": \"select * from market_data where symbol like 'TY%' and instrument_type='option' and date >= :start_date and date <= :end_date\",\n      \"params\": {\n        \"start_date\": \"2024-05-01\",\n        \"end_date\": \"2024-06-01\"\n      },\n      \"columns\": [\"date\", \"symbol\", \"bid\", \"ask\", \"last\", \"volume\"],\n      \"limit\": 5000\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Analyze and compare implied volatility levels derived from TY options data from last year versus recent month to determine if volatility is cheap now.\",\n    \"agent\": \"\",\n    \"params\": {\n      \"input_data_refs\": [\"task_1\", \"task_2\"],\n      \"method\": \"calculate implied vol proxies, summarize volatility trends, and compare current vs historical levels\"\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Retrieve trader messages from Bloomberg Chat and Telegram containing keywords 'vol', 'gamma', 'options' over the last month to extract sentiment on buying gamma in TY futures options.\",\n    \"agent\": \"message_puller_agent\",\n    \"params\": {\n      \"channels\": [\"Bloomberg Chat\", \"Telegram\"],\n      \"keywords\": [\"vol\", \"gamma\", \"options\", \"TY\", \"futures\"],\n      \"time_window\": {\n        \"start\": \"2024-05-01T00:00:00Z\",\n        \"end\": \"2024-06-01T00:00:00Z\"\n      }\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Synthesize data from implied volatility comparison and trader sentiment to provide an assessment if buying gamma in TY futures options currently makes sense.\",\n    \"agent\": \"\",\n    \"params\": {\n      \"inputs\": [\"task_3\", \"task_4\"],\n      \"goal\": \"Provide a reasoned conclusion on the attractiveness of buying gamma based on vol cheapness and market sentiment.\"\n    },\n    \"dependencies\": [\"task_3\", \"task_4\"]\n  }\n]"