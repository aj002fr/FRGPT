"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve all Nonfarm Payroll (NFP) event dates from 1990 to present with event dates and labels\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"NFP\",\n      \"count_or_range\": \"from 1990-01-01 to present\",\n      \"fields\": [\"date\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily closing prices for 10-year Treasury futures (TY) for all NFP event dates and the previous trading day to compute price moves\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 0]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute daily closing price moves on NFP Fridays by subtracting previous day close from NFP Friday close and classify each move as higher, lower, or unchanged\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"method\": \"compute_moves_and_classify\",\n      \"input_task\": \"task_2\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Summarize frequency counts of 10-year Treasury yield closing moves on NFP Fridays: number of higher closes, lower closes, and unchanged closes\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"method\": \"summarize_move_frequencies\",\n      \"input_task\": \"task_3\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Prepare data distribution for visualization of 10-year Treasury yield closing moves on NFP Fridays\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"method\": \"prepare_distribution_data\",\n      \"input_task\": \"task_3\"\n    },\n    \"dependencies\": [\"task_3\"]\n  }\n]"