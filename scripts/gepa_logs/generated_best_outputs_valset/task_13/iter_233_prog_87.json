"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve all NFP event dates (payroll Fridays) since 1990 with event dates.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"since 1990-01-01\",\n      \"event_type\": \"NFP\",\n      \"fields\": [\"date\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily closing prices of 10-year Treasury futures (TY) for NFP event dates plus prior day to compute daily changes.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"date\", \"price_close\"],\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 0]\n      },\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Calculate daily price changes on NFP dates by comparing TY closing price on the event day vs prior day; classify close as higher, lower, or unchanged.\",\n    \"agent\": \"local_processing\",\n    \"params\": {\n      \"input_data_task\": \"task_2\",\n      \"method\": \"calculate_change_and_classify\",\n      \"classification_rules\": {\n        \"higher\": \"price_close_today > price_close_prior_day\",\n        \"lower\": \"price_close_today < price_close_prior_day\",\n        \"unchanged\": \"price_close_today == price_close_prior_day\"\n      }\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Aggregate counts of higher, lower, and unchanged closes on payroll Fridays from classification results.\",\n    \"agent\": \"local_processing\",\n    \"params\": {\n      \"input_data_task\": \"task_3\",\n      \"method\": \"aggregate_classification_counts\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Visualize the distribution of 10-year Treasury futures price moves (daily changes) on payroll Fridays since 1990.\",\n    \"agent\": \"local_processing\",\n    \"params\": {\n      \"input_data_task\": \"task_3\",\n      \"method\": \"plot_distribution\",\n      \"plot_type\": \"histogram\",\n      \"x_axis_label\": \"Price Change (Close-to-Close)\",\n      \"title\": \"Distribution of 10Y Treasury Futures Price Moves on Payroll Fridays Since 1990\"\n    },\n    \"dependencies\": [\"task_3\"]\n  }\n]"