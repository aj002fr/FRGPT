"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve all U.S. Nonfarm Payroll (NFP) event dates since 1990\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"NFP\",\n      \"count_or_range\": {\"start_date\": \"1990-01-01\", \"end_date\": \"today\"},\n      \"fields\": [\"date\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve 10-year Treasury daily close prices for all NFP payroll Fridays and the prior trading day to compute price change\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\", \n      \"fields\": [\"date\", \"close_price\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 0]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute 10Y yield close price moves on payroll Fridays: classify as higher, lower or unchanged versus prior day\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"computation\": \"calculate price difference between payroll Friday close and previous close; classify move direction\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Summarize frequency counts of 10Y yield close moves (higher, lower, unchanged) on payroll Fridays\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"computation\": \"aggregate counts by move direction from task_3 output\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Prepare data for visualization of the distribution of 10Y yield close price moves on payroll Fridays\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"computation\": \"prepare distribution data (histogram/bar chart data) of price changes from task_3 output\"\n    },\n    \"dependencies\": [\"task_3\"]\n  }\n]"