"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve all U.S. Non-Farm Payroll (NFP) event dates from 1990 to present.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"NFP\",\n      \"count_or_range\": \"all since 1990-01-01\",\n      \"fields\": [\"date\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily closing prices for 10-year Treasury futures (TY) for all NFP event dates and the previous trading day.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"price_close\", \"timestamp\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 0]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Calculate the daily close price change for each NFP Friday: classify moves as higher, lower, or unchanged.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"compute price change direction and magnitude\",\n      \"input_data_task\": \"task_2\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Aggregate counts of higher, lower, and unchanged close moves on NFP Fridays since 1990.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"aggregate counts by classification\",\n      \"input_data_task\": \"task_3\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Generate visualization of the distribution of 10Y yield close price moves on NFP Fridays since 1990.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"plot histogram or density of price changes\",\n      \"input_data_task\": \"task_3\"\n    },\n    \"dependencies\": [\"task_3\"]\n  }\n]"