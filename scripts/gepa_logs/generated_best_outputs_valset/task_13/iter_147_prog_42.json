"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve all NFP event dates (payroll Fridays) in the US since 1990 with expected and actual values.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"NFP\",\n      \"count_or_range\": \"since 1990\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily closing prices for 10-year Treasury futures (TY) on NFP dates and the prior trading day to calculate daily moves.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"price_close\", \"date\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-1, 0]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Calculate daily price changes for TY on payroll Fridays relative to prior trading day and classify moves as higher, lower, or unchanged.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_prices_task\": \"task_2\",\n      \"event_dates_task\": \"task_1\"\n    },\n    \"dependencies\": [\"task_2\", \"task_1\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Summarize frequency counts of higher vs lower vs unchanged 10-year yield closes on payroll Fridays since 1990.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"classified_moves_task\": \"task_3\"\n    },\n    \"dependencies\": [\"task_3\"]\n  }\n]"