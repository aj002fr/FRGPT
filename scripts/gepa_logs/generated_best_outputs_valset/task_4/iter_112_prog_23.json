"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve all historical CPI events with actual CPI values and dates\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"CPI\",\n      \"fields\": [\"date\", \"actual\"],\n      \"count_or_range\": \"all historical\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Identify dates where CPI was above 4%\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"filter_condition\": \"actual > 4\",\n      \"source_task\": \"task_1\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Identify dates where CPI was below 2%\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"filter_condition\": \"actual < 2\",\n      \"source_task\": \"task_1\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Retrieve daily 10-year Treasury yield or futures price data for the full historical range covering all CPI events\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [0, 0]\n      },\n      \"frequency\": \"daily\",\n      \"instrument\": \"10Y Treasury futures or 10Y Treasury Note yield\",\n      \"fields\": [\"date\", \"closing_price_or_yield\"]\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Calculate daily changes in 10-year Treasury yields from the retrieved data\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"calculation\": \"daily change of closing_price_or_yield\",\n      \"source_task\": \"task_4\"\n    },\n    \"dependencies\": [\"task_4\"]\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Segment daily changes based on CPI above 4% dates\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"filter_dates\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"date\"\n      },\n      \"source_task\": \"task_5\"\n    },\n    \"dependencies\": [\"task_2\", \"task_5\"]\n  },\n  {\n    \"id\": \"task_7\",\n    \"description\": \"Segment daily changes based on CPI below 2% dates\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"filter_dates\": {\n        \"anchor_task\": \"task_3\",\n        \"anchor_field\": \"date\"\n      },\n      \"source_task\": \"task_5\"\n    },\n    \"dependencies\": [\"task_3\", \"task_5\"]\n  },\n  {\n    \"id\": \"task_8\",\n    \"description\": \"Prepare distribution data for charting comparing daily yield changes conditional on CPI >4% vs <2%\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"source_tasks\": [\"task_6\", \"task_7\"],\n      \"output_format\": \"distribution data suitable for histogram or density plot\"\n    },\n    \"dependencies\": [\"task_6\", \"task_7\"]\n  }\n]"