"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve historical CPI event data with actual values and dates for the entire available history.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"CPI\",\n      \"fields\": [\"date\", \"actual\"],\n      \"count_or_range\": \"all\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Filter CPI events into two groups: dates with CPI above 4% and dates with CPI below 2%. This is a data processing step using output from task_1.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"source_task\": \"task_1\",\n      \"filter_conditions\": [\n        {\"condition\": \"actual > 4%\", \"label\": \"CPI_above_4\"},\n        {\"condition\": \"actual < 2%\", \"label\": \"CPI_below_2\"}\n      ]\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily 10-year Treasury yield prices for days around CPI events with CPI above 4%. Date ranges anchored on filtered CPI event dates from task_2 for CPI_above_4 group.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y\",\n      \"fields\": [\"price\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"date\",\n        \"relative\": [0,0],\n        \"filter_label\": \"CPI_above_4\"\n      }\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Retrieve daily 10-year Treasury yield prices for days around CPI events with CPI below 2%. Date ranges anchored on filtered CPI event dates from task_2 for CPI_below_2 group.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y\",\n      \"fields\": [\"price\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"date\",\n        \"relative\": [0,0],\n        \"filter_label\": \"CPI_below_2\"\n      }\n    },\n    \"dependencies\": [\"task_2\"]\n  }\n]"