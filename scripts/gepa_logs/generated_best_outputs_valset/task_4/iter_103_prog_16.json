"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve historical CPI event dates and actual CPI values across history\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"CPI\",\n      \"fields\": [\"date\", \"actual\"],\n      \"count_or_range\": \"all\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Determine date range covering all CPI events from task_1\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"operation\": \"extract_date_range\",\n      \"source_task\": \"task_1\",\n      \"date_field\": \"date\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily 10-year Treasury yield prices for the date range covering all CPI events\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y\",\n      \"fields\": [\"price\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"date_range\",\n        \"relative\": [0, 0]\n      }\n    },\n    \"dependencies\": [\"task_2\"]\n  }\n]"