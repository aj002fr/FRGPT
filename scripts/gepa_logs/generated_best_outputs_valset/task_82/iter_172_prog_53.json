"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve last 10 CPI release events with actual and expected values, including surprise metrics and dates.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 10\",\n      \"event_type\": \"CPI\",\n      \"fields\": [\"date\", \"actual\", \"expected\", \"surprise\", \"surprise_label\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily 10-year Treasury futures (TY) prices for a 3-day window following each CPI release date obtained from task_1.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"price_close\", \"price_open\", \"price_high\", \"price_low\", \"volume\", \"timestamp\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [0, 3]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Analyze and log subsequent TY price moves relative to CPI actual vs expected prints and surprise metrics from task_1 and price data from task_2.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"event_data_task\": \"task_1\",\n      \"market_data_task\": \"task_2\"\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  }\n]"