"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve all FOMC meeting events from 1994 including dates, expected and actual rate decisions, and surprises to identify the key 1994 rate hike events that triggered the bond market selloff.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"all events during 1994\",\n      \"event_type\": \"FOMC\",\n      \"fields\": [\"date\", \"expected_rate\", \"actual_rate\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve 10-year Treasury futures prices (symbol TY) daily close prices for the period 3 months before the earliest 1994 FOMC meeting and 3 months after the latest 1994 FOMC meeting, using the FOMC events from task_1 as anchors. Document inverse relationship of price to yield.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"start\": {\n          \"anchor_task\": \"task_1\",\n          \"anchor_field\": \"date\",\n          \"offset_days\": -90,\n          \"aggregation\": \"min\"\n        },\n        \"end\": {\n          \"anchor_task\": \"task_1\",\n          \"anchor_field\": \"date\",\n          \"offset_days\": 90,\n          \"aggregation\": \"max\"\n        }\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Analyze the 10Y Treasury futures price data from task_2 to characterize how yields traded in the months leading up to and following the 1994 FOMC meetings, noting inverse price-yield relationship and key price movements indicating yield changes.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_task\": \"task_2\",\n      \"note\": \"Inverse relationship: as futures price falls, yield rises. Summarize price trends as yield behavior.\"\n    },\n    \"dependencies\": [\"task_2\"]\n  }\n]"