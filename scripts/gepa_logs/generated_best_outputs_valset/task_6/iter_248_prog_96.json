"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Identify the exact or approximate date of the 1994 bond market crash using web search agent.\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"1994 US bond market crash date\",\n      \"time_window\": \"historical\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily 10-year US Treasury yield or futures price data for several months before and after the 1994 bond market crash date identified in task_1.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y_Treasury_Future\",\n      \"fields\": [\"price\", \"yield\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-120, 120]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Plot the 10-year Treasury yield or price data retrieved in task_2 showing the months leading up to, during, and following the 1994 bond market crash.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"plot_type\": \"time_series\",\n      \"data_source_task\": \"task_2\",\n      \"fields\": [\"yield\", \"price\"]\n    },\n    \"dependencies\": [\"task_2\"]\n  }\n]"