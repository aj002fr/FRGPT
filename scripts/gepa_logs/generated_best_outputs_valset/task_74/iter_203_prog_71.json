"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Identify date ranges for major structural regime shifts: Volcker disinflation, 2008 crisis, post-COVID using web search.\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"dates and durations of major US Treasury market regime shifts including Volcker disinflation period, 2008 financial crisis period, and post-COVID economic period\",\n      \"time_window\": \"last 50 years\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily prices for 30-year Treasury futures over the last 30 years from market data.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"30Y Treasury futures\",\n      \"fields\": [\"last_price\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"start_date\": \"today_minus_30_years\",\n        \"end_date\": \"today\"\n      }\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Calculate daily changes in 30-year Treasury yields as proxy from futures prices.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_task\": \"task_2\",\n      \"calculation\": \"daily yield change proxy derived from daily futures price changes\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Label daily yield changes with regime shift periods identified from web search for overlay purposes.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_task_changes\": \"task_3\",\n      \"regime_date_ranges_task\": \"task_1\",\n      \"regime_periods\": [\"Volcker disinflation\", \"2008 crisis\", \"post-COVID\"]\n    },\n    \"dependencies\": [\"task_1\", \"task_3\"]\n  }\n]"