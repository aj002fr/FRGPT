"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Search Polymarket for markets related to 'U.S. CPI > 3% at year-end' to determine existence and get market IDs\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. CPI > 3% at year-end\",\n      \"limit\": 5\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Fetch historical price series for the identified Polymarket market(s) on 'U.S. CPI > 3% at year-end'\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. CPI > 3% at year-end\",\n      \"time_frame\": \"historical\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve headline CPI event data (dates, actual values, expected values, surprises) covering the timeframe of Polymarket price history\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"CPI\",\n      \"count_or_range\": \"last 12 months\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Fetch 5-year breakeven inflation market data (daily prices) for the same date range as Polymarket price history\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"5Y breakeven inflation\",\n      \"date_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"date_range\",\n        \"relative\": [0, 0]\n      },\n      \"fields\": [\"price\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": [\"task_2\"]\n  }\n]"