"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Search Polymarket for a market on 'U.S. CPI > 3% at year-end'\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. CPI > 3% at year-end\",\n      \"limit\": 5\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Fetch historical price series for the identified Polymarket market on 'U.S. CPI > 3% at year-end'\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. CPI > 3% at year-end\",\n      \"time_frame\": \"historical\",\n      \"limit\": 1000\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve 5-year breakeven inflation market data (daily prices) for the same date range as Polymarket historical prices\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"5Y breakeven inflation\",\n      \"fields\": [\"price\", \"volume\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"date_range\",\n        \"relative\": [0, 0]\n      }\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Retrieve headline CPI event data including actuals, expected, and surprises anchored to the Polymarket historical price date range\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"CPI\",\n      \"fields\": [\"date\", \"actual\", \"expected\", \"surprise\"],\n      \"count_or_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"date_range\",\n        \"relative\": [0, 0]\n      }\n    },\n    \"dependencies\": [\"task_2\"]\n  }\n]"