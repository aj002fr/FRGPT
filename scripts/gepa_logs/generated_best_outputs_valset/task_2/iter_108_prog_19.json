"[\n  {\n    \"id\": \"task_1_search_polymarket\",\n    \"description\": \"Search Polymarket markets for contracts on 'UST 10y yield above 5.5% before 2026'\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"UST 10y yield above 5.5% before 2026\",\n      \"limit\": 5\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2_get_cpi_events\",\n    \"description\": \"Retrieve recent CPI inflation events with dates, expected and actual values, and surprise labels\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"CPI\",\n      \"count_or_range\": \"last 10\",\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3_get_polymarket_historical_prices\",\n    \"description\": \"Fetch historical price series for the Polymarket market(s) discovered in task_1\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": null,\n      \"limit\": 1,\n      \"session_id\": null,\n      \"fields\": [\"historical_prices\"],\n      \"market_id_reference\": \"task_1_search_polymarket\"\n    },\n    \"dependencies\": [\"task_1_search_polymarket\"]\n  },\n  {\n    \"id\": \"task_4_analyze_price_changes_around_cpi\",\n    \"description\": \"Analyze Polymarket contract price changes around key CPI inflation print dates using historical prices from task_3 and CPI events from task_2\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"historical_prices_reference\": \"task_3_get_polymarket_historical_prices\",\n      \"event_dates_reference\": \"task_2_get_cpi_events\",\n      \"date_range\": {\n        \"anchor_task\": \"task_2_get_cpi_events\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-3, 3]\n      }\n    },\n    \"dependencies\": [\"task_2_get_cpi_events\", \"task_3_get_polymarket_historical_prices\"]\n  }\n]"