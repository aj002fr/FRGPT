"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve historical CPI event data including dates, core CPI actual values, and headline CPI actual values for the last year\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"CPI\",\n      \"count_or_range\": \"last 1 year\",\n      \"fields\": [\"date\", \"core_cpi_actual\", \"headline_cpi_actual\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"From CPI event data, classify event dates into two groups: core CPI > headline CPI and core CPI < headline CPI, outputting two date lists\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_from\": \"task_1\",\n      \"operation\": \"compare core_cpi_actual vs headline_cpi_actual and separate dates accordingly\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily TY futures prices for all dates identified in task_2 (both groups), including one day before and one day after each event date for return calculation\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"date_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"dates\",\n        \"relative\": [-1, 1]\n      },\n      \"frequency\": \"daily\",\n      \"fields\": [\"price\", \"date\"]\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate daily returns of TY futures for dates when core CPI > headline and when core CPI < headline to prepare data for histogram plotting\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_prices\": \"task_3\",\n      \"input_date_groups\": \"task_2\",\n      \"operation\": \"compute daily returns grouped by core CPI > headline and core CPI < headline\"\n    },\n    \"dependencies\": [\"task_3\", \"task_2\"]\n  }\n]"