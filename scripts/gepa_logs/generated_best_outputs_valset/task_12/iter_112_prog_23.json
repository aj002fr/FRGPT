"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve historical CPI event data with actual core CPI and headline CPI values including event dates\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"CPI\",\n      \"fields\": [\"date\", \"core_actual\", \"headline_actual\"],\n      \"count_or_range\": \"last 100 events\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Determine date range covering CPI events for TY price retrieval\",\n    \"agent\": \"internal_processing\",\n    \"params\": {\n      \"input_task\": \"task_1\",\n      \"operation\": \"find min and max event dates\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily TY futures price data covering date range of CPI events\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"date_range\",\n        \"relative\": [0, 0]\n      },\n      \"frequency\": \"daily\",\n      \"instrument\": \"TY\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate daily TY futures returns from price data\",\n    \"agent\": \"internal_processing\",\n    \"params\": {\n      \"input_task\": \"task_3\",\n      \"operation\": \"compute daily returns\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Label CPI events as core CPI > headline CPI or core CPI < headline CPI\",\n    \"agent\": \"internal_processing\",\n    \"params\": {\n      \"input_task\": \"task_1\",\n      \"operation\": \"classify events by core_actual > headline_actual or vice versa\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Join CPI labels with TY daily returns on event dates and separate returns based on core vs headline CPI condition\",\n    \"agent\": \"internal_processing\",\n    \"params\": {\n      \"input_tasks\": [\"task_4\", \"task_5\"],\n      \"operation\": \"merge returns on event dates with CPI classification and group returns accordingly\"\n    },\n    \"dependencies\": [\"task_4\", \"task_5\"]\n  },\n  {\n    \"id\": \"task_7\",\n    \"description\": \"Generate histogram data for daily TY returns when core CPI > headline CPI vs when core CPI < headline CPI\",\n    \"agent\": \"internal_processing\",\n    \"params\": {\n      \"input_task\": \"task_6\",\n      \"operation\": \"compute histogram bin counts or summary statistics for the two groups\"\n    },\n    \"dependencies\": [\"task_6\"]\n  }\n]"