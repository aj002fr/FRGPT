"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve upcoming FOMC meeting event data including dates, expected and actual rate decisions, and surprises to anchor the Polymarket query.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"FOMC\",\n      \"count_or_range\": \"next 1 upcoming event\",\n      \"fields\": [\"date\", \"expected_rate_decision\", \"actual_rate_decision\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Search Polymarket for prediction markets related to the Fed rate decision using natural language queries anchored on the upcoming FOMC event date.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"Federal Reserve rate decision\",\n      \"session_id\": \"fed_rate_decision_search\",\n      \"limit\": 5,\n      \"time_frame\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-7, 7]\n      }\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve current market data including prices, probabilities, volume, and liquidity for the top Polymarket markets related to the Fed rate decision identified in task_2.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"Federal Reserve rate decision\",\n      \"session_id\": \"fed_rate_decision_current_data\",\n      \"limit\": 5,\n      \"fields\": [\"price\", \"probability\", \"volume\", \"liquidity\"]\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Fetch historical price series (\u00b13 days) for the selected Polymarket markets to analyze recent trend evolution anchored on the upcoming FOMC event date.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"Federal Reserve rate decision\",\n      \"session_id\": \"fed_rate_decision_historical_data\",\n      \"limit\": 5,\n      \"time_frame\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-3, 3]\n      },\n      \"fields\": [\"historical_prices\", \"timestamps\"]\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Summarize the current Polymarket-implied probabilities for the Fed rate decision and analyze recent price trends around the FOMC event.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_tasks\": [\"task_3\", \"task_4\"]\n    },\n    \"dependencies\": [\"task_3\", \"task_4\"]\n  }\n]"