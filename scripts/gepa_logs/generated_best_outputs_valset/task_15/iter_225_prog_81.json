"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve recent and upcoming FOMC events with dates, expected and actual values, and surprise metrics to anchor Polymarket queries.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"FOMC\",\n      \"count_or_range\": \"last 5 events\",\n      \"fields\": [\"date\", \"expected_rate_decision\", \"actual_rate_decision\", \"surprise_direction\", \"surprise_magnitude\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Search Polymarket for markets related to the Federal Reserve's next policy action using the natural language query 'Fed'.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"Federal Reserve next rate decision or policy action\",\n      \"limit\": 5\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve current Polymarket prices, volumes, and liquidity for the markets identified in task_2.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"Federal Reserve next rate decision or policy action\",\n      \"fields\": [\"price\", \"volume\", \"liquidity\"],\n      \"limit\": 5\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Fetch historical price series for the Polymarket markets identified in task_2 for \u00b13 days around the dates of the FOMC events from task_1.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"Federal Reserve next rate decision or policy action\",\n      \"time_frame\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-3, 3]\n      },\n      \"limit\": 5\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Analyze repricing and trend evolution of Polymarket-implied probabilities for Fed policy action by comparing current vs historical market data from tasks 3 and 4.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"current_data_task\": \"task_3\",\n      \"historical_data_task\": \"task_4\",\n      \"event_data_task\": \"task_1\"\n    },\n    \"dependencies\": [\"task_3\", \"task_4\"]\n  }\n]"