"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve major macroeconomic event dates (CPI, NFP, FOMC, etc.) for the last 6 months\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 6 months\",\n      \"event_type\": [\"CPI\", \"NFP\", \"FOMC\", \"PCE\"],\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily closing prices for TY futures for the last 6 months\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": \"last 6 months\",\n      \"fields\": [\"last_price\"],\n      \"frequency\": \"daily\",\n      \"instrument\": \"TY\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute daily returns of TY futures and label days as event or no-event based on event dates\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"event_dates_task\": \"task_1\",\n      \"price_data_task\": \"task_2\"\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  }\n]"