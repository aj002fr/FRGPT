"[\n  {\n    \"task_id\": \"task_1\",\n    \"agent\": \"event_data_puller_agent\",\n    \"description\": \"Pull dates of major macro events (CPI, NFP, FOMC) for last 1 year to identify event days\",\n    \"dependencies\": [],\n    \"params\": {\n      \"count_or_range\": \"last 1 year\",\n      \"event_type\": \"CPI,NFP,FOMC\",\n      \"fields\": \"date,expected,actual,surprise\"\n    }\n  },\n  {\n    \"task_id\": \"task_2\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Retrieve TY futures daily closing prices covering the full date range of event data plus buffer for return calculation\",\n    \"dependencies\": [\"task_1\"],\n    \"params\": {\n      \"instrument\": \"TY*\",\n      \"frequency\": \"daily\",\n      \"fields\": \"date,close\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-10, 10]\n      }\n    }\n  },\n  {\n    \"task_id\": \"task_3\",\n    \"agent\": \"market_data_agent\",\n    \"description\": \"Calculate daily returns of TY futures from closing prices\",\n    \"dependencies\": [\"task_2\"],\n    \"params\": {\n      \"template\": \"calculate_daily_returns\",\n      \"params\": {\n        \"price_field\": \"close\",\n        \"date_field\": \"date\"\n      }\n    }\n  },\n  {\n    \"task_id\": \"task_4\",\n    \"agent\": \"distribution_wizard_agent\",\n    \"description\": \"Generate histograms of TY futures daily returns conditioned on days with major macro events vs days without events\",\n    \"dependencies\": [\"task_1\", \"task_3\"],\n    \"params\": {\n      \"data_sources\": {\n        \"returns\": \"task_3\",\n        \"event_dates\": \"task_1\"\n      },\n      \"conditioning\": {\n        \"event_days\": \"dates from task_1\",\n        \"non_event_days\": \"all other days in returns excluding event_days\"\n      },\n      \"field\": \"daily_return\",\n      \"plot_type\": \"histogram\",\n      \"goal\": \"Compare distributions of returns on event days vs non-event days\"\n    }\n  }\n]"