"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve dates of major macro events (CPI, NFP, FOMC) over the past year to identify big-ticket event days.\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"past 1 year\",\n      \"event_type\": [\"CPI\", \"NFP\", \"FOMC\"],\n      \"fields\": [\"date\", \"expected\", \"actual\", \"surprise\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily closing prices for TY futures over the same past 1 year period.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [0, 0]\n      },\n      \"frequency\": \"daily\",\n      \"instrument\": \"TY\",\n      \"fields\": [\"last_price\"]\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute daily returns for TY futures and classify each day as either having a big-ticket macro event or no major macro event based on event dates.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"event_dates_task\": \"task_1\",\n      \"price_data_task\": \"task_2\"\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Generate histogram data of TY futures daily returns conditioned on event days vs non-event days for visualization.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"returns_and_labels_task\": \"task_3\"\n    },\n    \"dependencies\": [\"task_3\"]\n  }\n]"