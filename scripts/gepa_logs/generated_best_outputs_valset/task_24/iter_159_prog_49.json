"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve dates of major macro events (CPI, NFP, FOMC) over the past year\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"count_or_range\": \"last 1 year\",\n      \"event_type\": [\"CPI\", \"NFP\", \"FOMC\"],\n      \"fields\": [\"date\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Determine date range covering all retrieved macro events with a buffer of 10 days before earliest and 10 days after latest event\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"derive_date_range\",\n      \"source_task\": \"task_1\",\n      \"buffer_days\": 10\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve daily closing prices for TY futures over the date range covering all major macro events plus buffer\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"frequency\": \"daily\",\n      \"fields\": [\"close_price\"],\n      \"date_range\": {\n        \"anchor_task\": \"task_2\",\n        \"anchor_field\": \"date_range\",\n        \"relative\": [0, 0]\n      }\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate daily returns of TY futures from closing prices\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"calculate_daily_returns\",\n      \"price_task\": \"task_3\",\n      \"price_field\": \"close_price\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Label days as event-days or non-event-days based on macro event dates\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"labeling_task\": \"task_4\",\n      \"event_dates_task\": \"task_1\",\n      \"label_type\": \"event_vs_nonevent\"\n    },\n    \"dependencies\": [\"task_4\", \"task_1\"]\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Prepare histogram data of TY futures daily returns conditioned on event-days vs non-event-days\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"histogram_task\": \"task_5\",\n      \"condition_field\": \"event_vs_nonevent\",\n      \"return_field\": \"daily_return\",\n      \"bins\": 30\n    },\n    \"dependencies\": [\"task_5\"]\n  }\n]"