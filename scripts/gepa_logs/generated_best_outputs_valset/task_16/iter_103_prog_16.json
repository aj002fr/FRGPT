"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve the date of the next upcoming FOMC meeting\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"FOMC\",\n      \"count_or_range\": \"next 1\",\n      \"fields\": [\"date\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve prediction market odds of a Fed cut at the next FOMC meeting from Polymarket\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"Fed rate cut at next FOMC meeting\",\n      \"time_frame\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-7, 7]\n      },\n      \"limit\": 5\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve current front-end Treasury futures pricing (e.g., 2-year Treasury futures) for the week leading up to the next FOMC meeting\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"2Y*\",\n      \"date_range\": {\n        \"anchor_task\": \"task_1\",\n        \"anchor_field\": \"date\",\n        \"relative\": [-7, -1]\n      },\n      \"fields\": [\"last_price\", \"bid\", \"ask\", \"volume\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Compare prediction market Fed cut odds with front-end futures implied probabilities and summarize any discrepancy\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"Compare Fed cut probability from prediction markets vs front-end futures pricing discrepancy\",\n      \"time_window\": \"past week\"\n    },\n    \"dependencies\": [\"task_2\", \"task_3\"]\n  }\n]"