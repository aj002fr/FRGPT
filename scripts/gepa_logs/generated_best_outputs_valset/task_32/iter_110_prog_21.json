"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve daily yield data for 2-year Treasury futures/notes from 1990-01-01 to present\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": {\"start\": \"1990-01-01\", \"end\": \"2024-06-01\"},\n      \"instrument\": \"2Y Treasury futures or notes\",\n      \"fields\": [\"price\", \"yield\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily yield data for 10-year Treasury futures/notes from 1990-01-01 to present\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": {\"start\": \"1990-01-01\", \"end\": \"2024-06-01\"},\n      \"instrument\": \"10Y Treasury futures or notes\",\n      \"fields\": [\"price\", \"yield\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Calculate daily 2s10s spread by subtracting 10Y yield from 2Y yield\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"calculation\": \"spread = 2Y_yield - 10Y_yield\",\n      \"input_data_tasks\": [\"task_1\", \"task_2\"],\n      \"output_fields\": [\"date\", \"2s10s_spread\"]\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate 1-month changes in 2s10s spread from daily spread data\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"input_data_task\": \"task_3\",\n      \"change_period_days\": 30,\n      \"output_fields\": [\"date\", \"1m_change_2s10s\"]\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Identify prior periods since 1990 when 2s10s spread was inverted (spread < 0)\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"input_data_task\": \"task_3\",\n      \"condition\": \"2s10s_spread < 0\",\n      \"output_fields\": [\"date\", \"inversion_flag\"]\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Retrieve historical recession dates from calendarchecker via web search for annotation\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"calendarchecker recession dates historical\",\n      \"time_window\": \"all time\"\n    },\n    \"dependencies\": []\n  }\n]"