"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve daily yields for 2-year and 10-year treasury instruments from 1990 to present\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": [\"1990-01-01\", \"today\"],\n      \"frequency\": \"daily\",\n      \"fields\": [\"price\"],\n      \"instrument\": [\"2Y\", \"10Y\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Compute daily 2s10s spread as 10Y yield minus 2Y yield from retrieved yields\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"computation\": \"spread\",\n      \"formula\": \"10Y - 2Y\",\n      \"input_task_ids\": [\"task_1\"]\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Calculate 1-month changes in the daily 2s10s spread\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"computation\": \"1-month changes\",\n      \"input_task_id\": \"task_2\",\n      \"period_days\": 21\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Identify periods where the 2s10s spread is inverted (negative values)\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"condition\": \"spread < 0\",\n      \"input_task_id\": \"task_2\"\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Retrieve recession dates from calendarchecker for annotation\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"Recession dates site:calendarchecker.com\",\n      \"time_window\": \"all\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Prepare combined data for plotting: 1-month spread changes, inversion periods, and recession annotations\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"inputs\": {\n        \"spread_changes_task\": \"task_3\",\n        \"inversion_task\": \"task_4\",\n        \"recession_dates_task\": \"task_5\"\n      }\n    },\n    \"dependencies\": [\"task_3\", \"task_4\", \"task_5\"]\n  }\n]"