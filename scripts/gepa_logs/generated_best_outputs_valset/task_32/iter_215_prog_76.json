"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve daily closing prices for 2-year Treasury futures (symbol US) from 1990-01-01 to present.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"US\",\n      \"fields\": [\"price_close\", \"timestamp\"],\n      \"frequency\": \"daily\",\n      \"date_range\": [\"1990-01-01\", \"present\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily closing prices for 10-year Treasury futures (symbol TY) from 1990-01-01 to present.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"TY\",\n      \"fields\": [\"price_close\", \"timestamp\"],\n      \"frequency\": \"daily\",\n      \"date_range\": [\"1990-01-01\", \"present\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute the daily 2s10s spread as 10-year price minus 2-year price using data from tasks 1 and 2.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_2y_data\": \"task_1\",\n      \"input_10y_data\": \"task_2\"\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate 1-month changes in the 2s10s spread (difference between spread values 1 month apart) from task 3 output.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"spread_data\": \"task_3\",\n      \"change_period_days\": 21\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Identify periods when the 2s10s spread was inverted (spread < 0) from task 3 output.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"spread_data\": \"task_3\",\n      \"inversion_threshold\": 0\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Retrieve recession date ranges from calendarchecker using web search from 1990 to present.\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"calendarchecker recession dates 1990 to present\",\n      \"time_window\": [\"1990-01-01\", \"present\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_7\",\n    \"description\": \"Annotate identified 2s10s inversion periods from task 5 with recession dates from task 6.\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"inversion_periods\": \"task_5\",\n      \"recession_dates\": \"task_6\"\n    },\n    \"dependencies\": [\"task_5\", \"task_6\"]\n  }\n]"