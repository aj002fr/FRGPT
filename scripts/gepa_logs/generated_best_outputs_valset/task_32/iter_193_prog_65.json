"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve daily closing prices for 2-year Treasury futures from 1990 to present\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"2Y\",\n      \"fields\": [\"close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"start_date\": \"1990-01-01\",\n        \"end_date\": \"latest\"\n      }\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily closing prices for 10-year Treasury futures from 1990 to present\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y\",\n      \"fields\": [\"close\"],\n      \"frequency\": \"daily\",\n      \"date_range\": {\n        \"start_date\": \"1990-01-01\",\n        \"end_date\": \"latest\"\n      }\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Calculate daily 2s10s spread from 2Y and 10Y futures prices and convert prices to yield proxy\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_tasks\": [\"task_1\", \"task_2\"],\n      \"calculation\": \"Compute spread as yield proxy difference: yield ~ inverse of price; spread = (inverse price 2Y) - (inverse price 10Y)\"\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate distribution of 1-month (approx. 21 trading days) changes in 2s10s spread since 1990\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_task\": \"task_3\",\n      \"calculation\": \"Compute difference in 2s10s spread between t and t-21 trading days to get 1-month changes; summarize distribution\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Retrieve recession dates from calendarchecker via web search\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"recession dates calendarchecker USA historical\",\n      \"time_window\": \"all\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Identify prior times since 1990 when 2s10s spread was inverted at least as much as current inversion\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"input_task\": \"task_3\",\n      \"method\": \"Find dates with 2s10s spread below current inversion level\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_7\",\n    \"description\": \"Annotate identified inversion dates with recession periods retrieved from calendarchecker\",\n    \"agent\": \"none\",\n    \"params\": {\n      \"inversion_dates_task\": \"task_6\",\n      \"recession_dates_task\": \"task_5\"\n    },\n    \"dependencies\": [\"task_5\", \"task_6\"]\n  }\n]"