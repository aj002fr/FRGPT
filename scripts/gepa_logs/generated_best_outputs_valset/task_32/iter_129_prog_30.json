"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve daily price or yield data for 2-year Treasury from 1990 to present\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": {\n        \"start\": \"1990-01-01\",\n        \"end\": \"latest\"\n      },\n      \"instrument\": \"2Y\",\n      \"fields\": [\"price\", \"yield\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily price or yield data for 10-year Treasury from 1990 to present\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"date_range\": {\n        \"start\": \"1990-01-01\",\n        \"end\": \"latest\"\n      },\n      \"instrument\": \"10Y\",\n      \"fields\": [\"price\", \"yield\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Compute daily 2s10s spread from 2Y and 10Y yields\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"computation\": \"spread\",\n      \"spread_definition\": \"10Y yield minus 2Y yield\",\n      \"input_tasks\": [\"task_1\", \"task_2\"]\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Calculate 1-month changes in the 2s10s spread (approx. 21 trading days)\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"input_task\": \"task_3\",\n      \"change_period_days\": 21,\n      \"change_type\": \"difference\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Identify prior periods when the 2s10s spread was inverted (spread < 0)\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"input_task\": \"task_3\",\n      \"condition\": \"spread < 0\"\n    },\n    \"dependencies\": [\"task_3\"]\n  },\n  {\n    \"id\": \"task_6\",\n    \"description\": \"Retrieve recession dates from calendarchecker or equivalent source via web search\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"recession dates site:calendarchecker.com OR authoritative US recession calendar\",\n      \"time_window\": \"all\"\n    },\n    \"dependencies\": []\n  }\n]"