"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve daily yields or prices for 2-year Treasury futures from 1990-01-01 to present.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"2Y\",\n      \"date_range\": {\n        \"start\": \"1990-01-01\",\n        \"end\": \"today\"\n      },\n      \"fields\": [\"price\", \"yield\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Retrieve daily yields or prices for 10-year Treasury futures from 1990-01-01 to present.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"instrument\": \"10Y\",\n      \"date_range\": {\n        \"start\": \"1990-01-01\",\n        \"end\": \"today\"\n      },\n      \"fields\": [\"price\", \"yield\"],\n      \"frequency\": \"daily\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Calculate daily 2s10s spread as 2Y yield minus 10Y yield and compute 1-month (21 trading days) changes in the spread, identify inverted periods (spread < 0).\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"operation\": \"calculate_spread_and_change\",\n      \"inputs\": {\n        \"yield_2Y_task\": \"task_1\",\n        \"yield_10Y_task\": \"task_2\"\n      },\n      \"change_period_days\": 21\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Retrieve US recession dates from calendarchecker or equivalent authoritative source via web search.\",\n    \"agent\": \"web_search_agent\",\n    \"params\": {\n      \"query\": \"US recession dates site:calendarchecker.com OR 'US recession dates'\",\n      \"time_window\": \"all\"\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_5\",\n    \"description\": \"Prepare final dataset combining 1-month 2s10s changes, inverted period flags, and recession date annotations for plotting.\",\n    \"agent\": \"market_data_agent\",\n    \"params\": {\n      \"inputs\": {\n        \"spread_changes_task\": \"task_3\",\n        \"recession_dates_task\": \"task_4\"\n      },\n      \"operation\": \"annotate_and_format_for_plot\"\n    },\n    \"dependencies\": [\"task_3\", \"task_4\"]\n  }\n]"