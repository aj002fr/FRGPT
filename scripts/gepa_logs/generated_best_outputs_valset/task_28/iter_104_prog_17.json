"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Search Polymarket for prediction market contracts about 'fed cutting rates at the next meeting'.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"fed cutting rates at the next meeting\",\n      \"limit\": 5\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Fetch current prices and volumes for the discovered Polymarket contracts about Fed rate cuts.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"fed cutting rates at the next meeting\",\n      \"fields\": [\"prices\", \"volumes\"],\n      \"limit\": 5\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Fetch historical price series for the Polymarket contracts about Fed rate cuts to analyze changes over time.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"fed cutting rates at the next meeting\",\n      \"time_frame\": \"last 30 days\",\n      \"limit\": 5\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Compare current vs historical prices to analyze the change in implied probability of Fed rate cut contracts.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"fed cutting rates at the next meeting\"\n    },\n    \"dependencies\": [\"task_2\", \"task_3\"]\n  }\n]"