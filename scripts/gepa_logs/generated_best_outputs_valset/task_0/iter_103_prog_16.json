"[\n  {\n    \"id\": \"task_1\",\n    \"description\": \"Retrieve the date of the last FOMC meeting (the most recent one before today).\",\n    \"agent\": \"event_data_puller_agent\",\n    \"params\": {\n      \"event_type\": \"FOMC\",\n      \"count_or_range\": \"last 1\",\n      \"fields\": [\"date\"]\n    },\n    \"dependencies\": []\n  },\n  {\n    \"id\": \"task_2\",\n    \"description\": \"Search Polymarket for currently active prediction markets related to 'U.S. 10y yield above 5%' or similar yield topics.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. 10 year Treasury yield above 5% or similar\",\n      \"limit\": 5\n    },\n    \"dependencies\": [\"task_1\"]\n  },\n  {\n    \"id\": \"task_3\",\n    \"description\": \"Retrieve current prices, volumes, and liquidity data for the discovered relevant Polymarket contracts.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. 10 year Treasury yield above 5% or similar\",\n      \"fields\": [\"current_prices\", \"volume\", \"liquidity\"]\n    },\n    \"dependencies\": [\"task_2\"]\n  },\n  {\n    \"id\": \"task_4\",\n    \"description\": \"Fetch historical price series for the relevant Polymarket contracts starting from the last FOMC meeting date to present to analyze odds evolution.\",\n    \"agent\": \"polymarket_agent\",\n    \"params\": {\n      \"query\": \"U.S. 10 year Treasury yield above 5% or similar\",\n      \"time_frame\": {\n        \"start_date_anchor_task\": \"task_1\",\n        \"start_date_anchor_field\": \"date\",\n        \"end_date\": \"now\"\n      }\n    },\n    \"dependencies\": [\"task_1\", \"task_2\"]\n  }\n]"